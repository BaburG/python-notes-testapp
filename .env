# .env
# Environment variables for the FastAPI Notes app

# Option 1: Define the full DATABASE_URL (Recommended)
DATABASE_URL=postgresql://username:password@host:port/database_name
# Example:
#DATABASE_URL=postgresql://notes_user:secretpassword@localhost:5432/notesdb

# Option 2: Define individual components (if DATABASE_URL is not set)
#DB_HOST=localhost
#DB_PORT=5432
#DB_USER=postgres
#DB_PASSWORD=password
#DB_NAME=notesdb

# Application settings (Optional)
# LOG_LEVEL=INFO 